	<profiles>
		<profile>
		  <id>downloadSources</id>
		  <properties>
			<downloadSources>true</downloadSources>
			<downloadJavadocs>true</downloadJavadocs>
		  </properties>
		</profile>
		<profile>
			<id>default</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>bea.release</id>
					<url>http://nexus.arrs.arrisi.com/content/repositories/releases</url>
					<name>Beaverton Nexus Releases Repository</name>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>bea.snapshot</id>
					<url>http://nexus.arrs.arrisi.com/content/repositories/snapshots</url>
					<name>Beaverton Nexus Snapshots Repository</name>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<!-- never, daily, interval:X (where X is in minutes) or always Default: daily-->
						<updatePolicy>always</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>bea.public</id>
					<url>http://nexus.arrs.arrisi.com/content/groups/public</url>
					<name>Beaverton Nexus Public Repository</name>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>bea.public-snapshots</id>
					<url>http://nexus.arrs.arrisi.com/content/groups/public-snapshots</url>
					<name>Beaverton Nexus Public Snapshots Repository</name>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>eco-nexus</id>
					<url>http://nexus.arrs.arrisi.com/content/groups/public</url>
					<name>Beaverton Nexus Public Repository</name>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				
				<repository>
					<id>eco.bea.public</id>
					<url>https://nexus.arriseco.com/content/groups/public</url>
					<name>ARRIS Cloud Nexus Public Repository</name>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>eco.bea.release</id>
					<url>https://nexus.arriseco.com/content/repositories/releases</url>
					<name>ARRIS Cloud Nexus Releases Repository</name>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>eco.bea.snapshot</id>
